query CharacterDetailsList(){
characters(characters: $characters) {
        info{
          count,
          pages
        },
        results{
          id,
          name,
          status,
          species,
          gender,
          image,
          location{
            name
          },
          origin{
            name
          },
          episode{
            name,
            episode
          }
        }
      }

}